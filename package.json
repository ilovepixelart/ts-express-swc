{
  "name": "ts-express-swc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "swc src -d dist",
    "build:ts": "tsc --project tsconfig.json",
    "dev": "nodemon --ext ts --exec node --inspect -r @swc/register src/bin/www.ts",
    "lint": "yarn eslint ./src --fix . --ext .js,.ts",
    "lint:fix": "yarn eslint ./src --fix . --ext .js,.ts --fix",
    "test": "jest --config jest.config.ts --detectOpenHandles --coverage",
    "test:open": "npm run test && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "cookie-parser": "1.4.6",
    "debug": "4.3.4",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "http-errors": "2.0.0",
    "mongoose": "6.10.5",
    "morgan": "1.10.0",
    "pug": "3.0.2",
    "ts-migrate-mongoose": "3.1.2",
    "ts-patch-mongoose": "1.2.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "4.1.7",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.52",
    "@swc/register": "0.1.10",
    "@swc/helpers": "0.5.0",
    "@swc/jest": "0.2.26",
    "@types/jest": "29.5.1",
    "@types/cookie-parser": "1.4.3",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.17",
    "@types/http-errors": "2.0.1",
    "@types/morgan": "1.9.4",
    "@types/node": "18.15.12",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "eslint": "8.38.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.5.0",
    "merge": "2.1.1",
    "open-cli": "7.2.0",
    "nodemon": "2.0.22",
    "typescript": "5.0.4"
  }
}
