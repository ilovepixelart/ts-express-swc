{
  "name": "ts-express-swc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "swc src -d dist",
    "build:ts": "tsc --project tsconfig.json",
    "dev": "nodemon --ext ts --exec node --inspect -r @swc-node/register src/bin/www.ts",
    "biome": "npx @biomejs/biome check",
    "biome:fix": "npx @biomejs/biome check --write .",
    "test": "jest --config jest.config.ts --detectOpenHandles --coverage",
    "test:open": "npm run test && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "cookie-parser": "^1.4.7",
    "debug": "4.3.7",
    "dotenv": "16.4.5",
    "express": "^4.21.1",
    "http-errors": "2.0.0",
    "mongoose": "8.8.0",
    "morgan": "1.10.0",
    "pug": "3.0.3",
    "ts-cache-mongoose": "1.6.6",
    "ts-migrate-mongoose": "3.8.5",
    "ts-patch-mongoose": "2.6.6"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "4.3.2",
    "@swc-node/jest": "1.8.12",
    "@swc-node/register": "1.10.9",
    "@types/cookie-parser": "1.4.7",
    "@types/debug": "4.1.12",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.14",
    "@types/morgan": "1.9.9",
    "@types/node": "22",
    "jest": "29.7.0",
    "merge": "2.1.1",
    "nodemon": "3.1.7",
    "open-cli": "8.0.0",
    "typescript": "5.6.3"
  }
}
