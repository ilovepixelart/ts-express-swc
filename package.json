{
  "name": "ts-express-tsx",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "tsc",
    "dev": "tsx watch src/bin/www.ts",
    "biome": "npx @biomejs/biome check",
    "biome:fix": "npx @biomejs/biome check --write .",
    "test": "vitest run --coverage",
    "test:open": "npm test && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@biomejs/biome": "1.9.4",
    "cookie-parser": "1.4.7",
    "debug": "4.4.0",
    "dotenv": "16.4.7",
    "express": "5.0.1",
    "http-errors": "2.0.0",
    "mongoose": "8.12.1",
    "morgan": "1.10.0",
    "pug": "3.0.3",
    "ts-cache-mongoose": "1.7.2",
    "ts-migrate-mongoose": "4.0.6",
    "ts-patch-mongoose": "2.8.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.8",
    "@types/debug": "4.1.12",
    "@types/express": "5.0.0",
    "@types/http-errors": "2.0.4",
    "@types/morgan": "1.9.9",
    "@types/node": "22.13.10",
    "@vitest/coverage-v8": "3.0.8",
    "mongodb-memory-server": "10.1.4",
    "open-cli": "8.0.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "vitest": "3.0.8"
  }
}
