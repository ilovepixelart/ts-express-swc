{
  "name": "ts-express-swc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "swc src -d dist",
    "build:ts": "tsc --project tsconfig.json",
    "dev": "nodemon --ext ts --exec node --inspect -r @swc/register src/bin/www.ts",
    "lint": "yarn eslint ./src --fix . --ext .js,.ts",
    "lint:fix": "yarn eslint ./src --fix . --ext .js,.ts --fix",
    "test": "jest --config jest.config.ts --detectOpenHandles --coverage",
    "test:open": "npm run test && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "cookie-parser": "1.4.6",
    "debug": "4.3.4",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "http-errors": "2.0.0",
    "mongoose": "7.4.3",
    "morgan": "1.10.0",
    "pug": "3.0.2",
    "ts-cache-mongoose": "1.1.1",
    "ts-migrate-mongoose": "3.3.1",
    "ts-patch-mongoose": "2.2.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "4.1.7",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.76",
    "@swc/helpers": "0.5.1",
    "@swc/jest": "0.2.29",
    "@swc/register": "0.1.10",
    "@types/cookie-parser": "1.4.3",
    "@types/debug": "4.1.8",
    "@types/express": "4.17.17",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.5.3",
    "@types/morgan": "1.9.4",
    "@types/node": "20",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.4.1",
    "eslint": "8.47.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-sonarjs": "0.20.0",
    "jest": "29.6.2",
    "merge": "2.1.1",
    "nodemon": "3.0.1",
    "open-cli": "7.2.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
