{
  "name": "ts-express-swc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "swc src -d dist",
    "build:ts": "tsc --project tsconfig.json",
    "dev": "nodemon --ext ts --exec node --inspect -r @swc/register src/bin/www.ts",
    "lint": "yarn eslint ./src --fix . --ext .js,.ts",
    "lint:fix": "yarn eslint ./src --fix . --ext .js,.ts --fix",
    "test": "jest --config jest.config.ts --detectOpenHandles --coverage",
    "test:open": "npm run test && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "cookie-parser": "1.4.6",
    "debug": "4.3.4",
    "dotenv": "16.3.2",
    "express": "4.18.2",
    "http-errors": "2.0.0",
    "mongoose": "8.1.0",
    "morgan": "1.10.0",
    "pug": "3.0.2",
    "ts-cache-mongoose": "1.4.5",
    "ts-migrate-mongoose": "3.5.7",
    "ts-patch-mongoose": "2.5.4"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "4.2.0",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.104",
    "@swc/helpers": "0.5.3",
    "@swc/jest": "0.2.31",
    "@swc/register": "0.1.10",
    "@types/cookie-parser": "1.4.6",
    "@types/debug": "4.1.12",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.11",
    "@types/morgan": "1.9.9",
    "@types/node": "20",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.56.0",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "jest": "29.7.0",
    "merge": "2.1.1",
    "nodemon": "3.0.3",
    "open-cli": "8.0.0",
    "typescript": "5.3.3"
  }
}
