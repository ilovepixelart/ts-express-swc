{
  "name": "ts-express-swc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "swc src -d dist",
    "build:ts": "tsc --project tsconfig.json",
    "dev": "nodemon --ext ts --exec node --inspect -r @swc/register src/bin/www.ts",
    "lint": "eslint ./src --fix . --ext .js,.ts",
    "lint:fix": "eslint ./src --fix . --ext .js,.ts --fix",
    "test": "jest --config jest.config.ts --detectOpenHandles --coverage",
    "test:open": "npm run test && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "cookie-parser": "1.4.6",
    "debug": "4.3.4",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "http-errors": "2.0.0",
    "mongoose": "8.2.4",
    "morgan": "1.10.0",
    "pug": "3.0.2",
    "ts-cache-mongoose": "1.5.0",
    "ts-migrate-mongoose": "3.6.4",
    "ts-patch-mongoose": "2.6.0",
    "tslib": "2.6.2"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "4.2.0",
    "@stylistic/eslint-plugin": "1.7.0",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.4.13",
    "@swc/helpers": "0.5.8",
    "@swc/jest": "0.2.36",
    "@swc/register": "0.1.10",
    "@types/cookie-parser": "1.4.7",
    "@types/debug": "4.1.12",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.12",
    "@types/morgan": "1.9.9",
    "@types/node": "20",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.5.0",
    "eslint": "8.57.0",
    "eslint-plugin-jest": "28.2.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-sonarjs": "0.25.1",
    "jest": "29.7.0",
    "merge": "2.1.1",
    "nodemon": "3.1.0",
    "open-cli": "8.0.0",
    "typescript": "5.4.4"
  }
}
