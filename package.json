{
  "name": "ts-express-swc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "swc src -d dist",
    "build:ts": "tsc --project tsconfig.json",
    "dev": "nodemon --ext ts --exec node --inspect -r @swc/register src/bin/www.ts",
    "lint": "yarn eslint ./src --fix . --ext .js,.ts",
    "lint:fix": "yarn eslint ./src --fix . --ext .js,.ts --fix"
  },
  "dependencies": {
    "cookie-parser": "1.4.6",
    "debug": "4.3.4",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "http-errors": "2.0.0",
    "mongoose": "6.10.0",
    "morgan": "1.10.0",
    "pug": "3.0.2",
    "ts-migrate-mongoose": "^2.5.3"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "4.1.7",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.41",
    "@swc/register": "0.1.10",
    "@types/cookie-parser": "1.4.3",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.17",
    "@types/http-errors": "2.0.1",
    "@types/morgan": "1.9.4",
    "@types/node": "18.15.3",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "eslint": "8.36.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "merge": "2.1.1",
    "nodemon": "2.0.21",
    "typescript": "5.0.2"
  }
}
